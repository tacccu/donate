<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/TLDonaciones">

    <fragment
        android:id="@+id/perfil"
        android:name="cat.copernic.donate.perfil"
        android:label="fragment_perfil"
        tools:layout="@layout/fragment_perfil" >
        <action
            android:id="@+id/action_perfil_to_TLDonaciones"
            app:destination="@id/TLDonaciones" />
    </fragment>
    <fragment
        android:id="@+id/chatList"
        android:name="cat.copernic.donate.chatList"
        android:label="fragment_chat_list"
        tools:layout="@layout/fragment_chat_list" >
        <action
            android:id="@+id/action_chatList_to_TLDonaciones"
            app:destination="@id/TLDonaciones" />
    </fragment>
    <fragment
        android:id="@+id/TLDonaciones"
        android:name="cat.copernic.donate.TLDonaciones"
        android:label="fragment_tldonaciones"
        tools:layout="@layout/fragment_tldonaciones" >
        <action
            android:id="@+id/action_TLDonaciones_to_tltickets"
            app:destination="@id/tltickets" />
        <action
            android:id="@+id/action_TLDonaciones_to_chatList"
            app:destination="@id/chatList" />
        <action
            android:id="@+id/action_TLDonaciones_to_perfil"
            app:destination="@id/perfil" />
    </fragment>
    <fragment
        android:id="@+id/tltickets"
        android:name="cat.copernic.donate.tltickets"
        android:label="fragment_tltickets"
        tools:layout="@layout/fragment_tltickets" >
        <action
            android:id="@+id/action_tltickets_to_TLDonaciones"
            app:destination="@id/TLDonaciones" />
    </fragment>
</navigation>